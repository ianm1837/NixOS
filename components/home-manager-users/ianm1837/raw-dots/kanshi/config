profile Laptop-only {
    output eDP-1 enable scale 1.6 mode 2560x1600@165
}

profile one-monitor-a {
    output eDP-1 disable
    output DP-1 enable position 0,0
}

profile one-monitor-b {
    output eDP-1 disable
    output DP-2 enable position 0,0
}

profile both-monitors {
    output eDP-1 disable
    output "GIGA-BYTE TECHNOLOGY CO., LTD. Gigabyte M32Q 0x00000583" enable position 0,0
    output "GIGA-BYTE TECHNOLOGY CO., LTD. Gigabyte M32Q 0x00000228" enable position 2560,0
}